{
  "name": "console.freelog.com",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint --fix",
    "dev": "vue-cli-service serve",
    "build:prod": "npm run build -- --env=prod",
    "build:beta": "npm run build -- --env=beta",
    "pack": "tar -zcf console.tar dist",
    "test:e2e": "vue-cli-service test:e2e --config ./nightwatch.conf.js",
    "test:e2e:chrome": "npm run test:e2e -- --env chrome",
    "test:e2e:safari": "npm run test:e2e -- --env safari",
    "test:e2e:firefox": "npm run test:e2e -- --env firefox",
    "test:e2e:all": "npm run test:e2e -- --env chrome,firefox,safari"
  },
  "dependencies": {
    "@freelog/freelog-ui-contract": "0.0.9",
    "@freelog/resource-policy-lang": "0.0.33",
    "@vue/component-compiler-utils": "^2.4.0",
    "axios": "^0.18.0",
    "codemirror": "^5.42.2",
    "cropperjs": "^1.4.3",
    "date-fns": "^1.30.1",
    "element-ui": "^2.4.11",
    "font-awesome": "^4.7.0",
    "lodash": "^4.17.10",
    "lozad": "^1.7.0",
    "nprogress": "^0.2.0",
    "quill": "^1.3.6",
    "screenfull": "^3.3.3",
    "store": "^2.0.12",
    "vue": "^2.5.21",
    "vue-codemirror": "^4.0.6",
    "vue-i18n": "^8.7.0",
    "vue-lazyload": "^1.2.6",
    "vue-quill-editor": "^3.0.6",
    "vue-router": "^3.0.2",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@freelog/freelog-ci": "0.0.1",
    "@vue/cli-plugin-babel": "^3.2.2",
    "@vue/cli-plugin-e2e-nightwatch": "^5.0.8",
    "@vue/cli-plugin-eslint": "^3.2.2",
    "@vue/cli-service": "^3.2.3",
    "@vue/eslint-config-airbnb": "^3.0.5",
    "ali-oss": "^6.1.0",
    "chai": "^4.2.0",
    "chromedriver": "^2.45.0",
    "co": "^4.6.0",
    "consola": "^1.4.5",
    "geckodriver": "^1.14.1",
    "husky": "^1.3.1",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "ora": "^3.0.0",
    "selenium-server": "^3.141.59",
    "user-home": "^2.0.0",
    "vue-template-compiler": "^2.5.21",
    "walk": "^2.3.14"
  },
  "husky": {
    "hooks": {
      "pre-push": "freelog-ci deploy"
    }
  }
}
